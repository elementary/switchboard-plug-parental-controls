#: src/client/AppUnavailableDialog.vala:27
msgid "You are not permitted to run this application"
msgstr "Du får inte köra den här applikationen"

#: src/client/AppUnavailableDialog.vala:28
msgid "An administrator has restricted your access to this application."
msgstr "En administratör har begränsat din tillgång till denna applikation."

#: src/client/Client.vala:161
#, c-format
msgid "This computer will lock after %s."
msgstr "Den här datorn låses efter %s."

#: src/client/Client.vala:163
#, c-format
msgid "%lu hour"
msgid_plural "%lu hours"
msgstr[0] "%lu timme"
msgstr[1] "%lu timmar"

#: src/client/Client.vala:165
#, c-format
msgid "%lu minute"
msgid_plural "%lu minutes"
msgstr[0] "%lu minut"
msgstr[1] "%lu minuter"

#: src/client/Client.vala:167
msgid ""
"Make sure to close all applications before your computer will be locked."
msgstr "Glöm inte att stänga alla dina program innan datorn låser sig."

#: src/client/Client.vala:173
msgid "Time left"
msgstr "Återstående tid"

#: src/plug/Views/AppsView.vala:27
msgid "Blocked Apps"
msgstr "Blockerade appar"

#: src/plug/Views/AppsView.vala:38
msgid "Add Blocked App…"
msgstr "Lägg till blockerad app…"

#: src/plug/Views/AppsView.vala:53
msgid "Clear All"
msgstr "Töm allt"

#: src/plug/Views/AppsView.vala:77
msgid "Allow access to these apps with admin permission:"
msgstr "Tillåt åtkomst till dessa program med administratörsrättigheter:"

#: src/plug/Views/InternetView.vala:29
msgid "Blocked Websites"
msgstr "Blockerade webbplatser"

#: src/plug/Views/InternetView.vala:40
msgid "Block URL"
msgstr "Blockera URL"

#: src/plug/Views/InternetView.vala:50
msgid "example.com"
msgstr "exempel.se"

#: src/plug/Views/InternetView.vala:79 src/plug/Views/InternetView.vala:193
