msgstr "終了:"

#: src/plug/Widgets/UserList.vala:42
msgid "My Account"
msgstr "自分のアカウント"

#: src/plug/Widgets/UserList.vala:46
msgid "Other Accounts"
msgstr "その他のアカウント"

#: src/plug/Widgets/InternetBox.vala:77
#, c-format
msgid "Prevent %s from visiting these websites:"
msgstr "%s が以下の Web サイトを参照することを防ぐ:"

#: src/plug/Widgets/InternetBox.vala:90
msgid "Add URL"
msgstr "URL の追加"

#: src/plug/Widgets/InternetBox.vala:99
msgid "Add a new URL, for example: google.com"
msgstr "新しい URL を追加してください。例: google.com"

#: src/plug/Widgets/InternetBox.vala:100
msgid "Invalid URL"
msgstr "無効な URL"

#: src/plug/Widgets/GeneralBox.vala:62
msgid "Allow this user to:"
msgstr "ユーザーに許可する動作:"

#: src/plug/Widgets/GeneralBox.vala:65
msgid "Modify the dock"
msgstr "ドックの編集"

#: src/plug/Widgets/GeneralBox.vala:68
msgid "Configure printing"
msgstr "印刷の設定"

#: src/plug/Widgets/GeneralBox.vala:84
msgid "Limit computer use:"
msgstr "コンピューター利用の制限:"

#: src/plug/Widgets/GeneralBox.vala:92
msgid "On weekdays and weekends"
msgstr "平日と週末"

#: src/plug/Widgets/GeneralBox.vala:93
msgid "Only on weekdays"
msgstr "平日のみ"

#: src/plug/Widgets/GeneralBox.vala:94
msgid "Only on weekends"
msgstr "週末のみ"

#: src/plug/Widgets/GeneralBox.vala:104
msgid "Weekdays"
msgstr "平日"

#: src/plug/Widgets/GeneralBox.vala:108
msgid "Weekends"
msgstr "週末"

#: src/plug/Widgets/ControlPage.vala:53
msgid "General"
msgstr "一般"

#: src/plug/Widgets/ControlPage.vala:54
msgid "Internet"
msgstr "インターネット"

#: src/plug/Widgets/ControlPage.vala:55
msgid "Applications"
msgstr "アプリケーション"

#: src/plug/Widgets/AppsBox.vala:90
#, c-format
msgid "Prevent %s from using these apps:"
msgstr "%s が以下のアプリを使うことを防ぐ:"

#: src/plug/Widgets/AppsBox.vala:101
msgid "Add Prevented Apps…"
msgstr "制限するアプリの追加…"

#: src/plug/Widgets/AppsBox.vala:105
msgid "Remove Selected App"
msgstr "選択されたアプリを削除"

#: src/plug/Widgets/AppsBox.vala:110
msgid "Clear All"
msgstr "すべてをクリア"

#: src/plug/Widgets/AppsBox.vala:133
msgid "Allow access to these apps with admin permission:"
msgstr "管理者の許可でこれらのアプリにアクセスすることを許可:"

#: src/plug/Widgets/AppChooser.vala:71
msgid "Search Applications"
msgstr "アプリケーションを検索"

#: src/client/Client.vala:89
#, c-format
msgid "This computer will lock after %s."
msgstr "このコンピューターは %s 後にロックされます。"

#: src/client/Client.vala:91
#, c-format
msgid "%lu hour"
msgid_plural "%lu hours"
msgstr[0] "%lu 時間"

#: src/client/Client.vala:93
#, c-format
msgid "%lu minute"
msgid_plural "%lu minutes"
msgstr[0] "%lu 分"

#: src/client/Client.vala:95
msgid ""
"Make sure to close all applications before your computer will be locked."
msgstr ""
"コンピューターがロックされる前にすべてのアプリケーションを必ず閉じるようにし"
"てください。"

#: src/client/Client.vala:101
msgid "Time left"
msgstr "残り時間"

#: src/client/AppUnavailableDialog.vala:27
msgid "You cannot run this application"
msgstr "このアプリケーションを実行できません"

#: src/client/AppUnavailableDialog.vala:28
msgid "You are not permitted to run this application."
msgstr "このアプリケーションの実行は許可されていません。"

