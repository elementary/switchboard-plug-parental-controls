#: src/client/AppUnavailableDialog.vala:27
msgid "You are not permitted to run this application"
msgstr "Nemáte oprávnenie spúšťať túto aplikáciu"

#: src/client/AppUnavailableDialog.vala:28
msgid "An administrator has restricted your access to this application."
msgstr "Administrátor vám obmedzil prístup k tejto aplikácii."

#: src/client/Client.vala:158
#, c-format
msgid "This computer will lock after %s."
msgstr "Tento počítač sa uzamkne za %s."

#: src/client/Client.vala:160
#, c-format
msgid "%lu hour"
msgid_plural "%lu hours"
msgstr[0] "%lu hodín"
msgstr[1] "%lu hodinu"
msgstr[2] "%lu hodiny"

#: src/client/Client.vala:162
#, c-format
msgid "%lu minute"
msgid_plural "%lu minutes"
msgstr[0] "%lu minút"
msgstr[1] "%lu minútu"
msgstr[2] "%lu minúty"

#: src/client/Client.vala:164
msgid ""
"Make sure to close all applications before your computer will be locked."
msgstr ""
"Uistite sa, že ste zatvorili všetky aplikácie predtým, než bude váš počítač "
"uzamknutý."

#: src/client/Client.vala:170
msgid "Time left"
msgstr "Zostávajúci čas"

#: src/plug/Views/AppsView.vala:50
#, c-format
msgid "Prevent %s from using these apps:"
msgstr "Zabrániť %s používať tieto aplikácie:"

#: src/plug/Views/AppsView.vala:60
msgid "Add Prevented Apps…"
msgstr "Pridať obmedzené aplikácie..."

#: src/plug/Views/AppsView.vala:64
msgid "Remove Selected App"
msgstr "Odstrániť vybranú aplikáciu"

#: src/plug/Views/AppsView.vala:69
msgid "Clear All"
msgstr "Vymazať všetko"

#: src/plug/Views/AppsView.vala:92
msgid "Allow access to these apps with admin permission:"
msgstr "Povoliť prístup k týmto aplikáciam s povolením administrátora:"

#: src/plug/Views/InternetView.vala:72
#, c-format
msgid "Prevent %s from visiting these websites:"
msgstr "Zabrániť %s navštevovať tieto webstránky:"

#: src/plug/Views/InternetView.vala:87
msgid "Add URL"
msgstr "Pridať URL"

#: src/plug/Views/InternetView.vala:96
msgid "Add a new URL, for example: google.com"
msgstr "Pridajte novú URL, napríklad: google.com"

#: src/plug/Views/InternetView.vala:97
msgid "Invalid URL"
msgstr "Neplatná URL"

#. /TRANSLATORS: Refers to non-weekend days, as used in a title
#: src/plug/Views/TimeLimitView.vala:41
msgid "Weekdays"
msgstr "Pracovné dni"

#. /TRANSLATORS: Refers to weekend days, as used in a title
#: src/plug/Views/TimeLimitView.vala:49
msgid "Weekends"
msgstr "Víkendy"

